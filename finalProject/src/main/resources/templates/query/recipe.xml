<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.boot.recipe.dao.RecipeDao">

	<insert id="insertRecipeInfo" parameterType="RecipeVO">
		insert into recipe
		values(
			#{rcp_seq}, #{rcp_nm}, #{rcp_way2}, #{rcp_pat2}, #{info_eng},
		 	#{att_file_no_main}, #{att_file_no_mk}, #{rcp_parts_dtls}
		)
	</insert>
	<insert id="insertRecipeManual" parameterType="RecipeManualVO">
		insert into recipe_manual
		values(
			#{rcp_seq},
			#{manual01}, #{manual_img01}, #{manual02}, #{manual_img02}, #{manual03}, #{manual_img03}, #{manual04}, #{manual_img04},
			#{manual05}, #{manual_img05}, #{manual06}, #{manual_img06}, #{manual07}, #{manual_img07}, #{manual08}, #{manual_img08},
			#{manual09}, #{manual_img09}, #{manual10}, #{manual_img10}
		)
	</insert>
</mapper>